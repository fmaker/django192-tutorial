# .coveragerc to control coverage.py
[run]
omit = 
	*/migrations/*
	*/tests/*
	test_*.py
	*/__init__.py
	*/urls.py
	*/settings.py
	*/tests.py
source =
    mysite/polls

[report]
precision = 2
show_missing = True
ignore_errors = True

[html]
directory = coverage_report/
title = My Blog site test coverage